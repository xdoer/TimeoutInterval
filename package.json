  "module": "dist/timeout-interval.esm.js",
  "size-limit": [
    {
      "path": "dist/timeout-interval.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/timeout-interval.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.11.0",
    "husky": "^6.0.0",
    "size-limit": "^4.11.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2"
  }
}
